{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "import/prefer-default-export": "off",
        "semi":"off",
        "linebreak-style":"off",
        "import/extensions":"off",
        "import/no-unresolved":"off",
        "import/no-extraneous-dependencies":"off",
        "max-classes-per-file":"off",
        "no-unused-vars":"off",
        
    }
}